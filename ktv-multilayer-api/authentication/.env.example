# Configuration file untuk Earth Engine Service Accounts
# Sesuaikan dengan setup environment Anda

# Path ke folder authentication (relatif dari root project)
EE_SERVICE_ACCOUNT_PATH=authentication/

# Google Cloud Project ID (ganti dengan project ID Anda)
GOOGLE_CLOUD_PROJECT_ID=your-project-id-here

# Total number of service accounts untuk parallel processing
TOTAL_SERVICE_ACCOUNTS=16

# Earth Engine quota settings per account
EE_DAILY_QUOTA_PER_ACCOUNT=25000
EE_REQUESTS_PER_MINUTE=1000

# Production settings
ENABLE_PARALLEL_PROCESSING=true
DEFAULT_PARALLEL_ACCOUNTS=16
FALLBACK_TO_SINGLE_ACCOUNT=true

# Database settings (jika menggunakan PostgreSQL + PostGIS)
# Uncomment jika diperlukan untuk production batch processing
# postgisHost=localhost
# postgisPort=5432
# postgisDatabase=eudr_compliance
# postgisUsername=postgres
# postgisPassword=your-password

# Monitoring & Logging
LOG_LEVEL=INFO
ENABLE_EE_QUOTA_MONITORING=true

# Security settings
ROTATE_CREDENTIALS_DAYS=90
ENABLE_IP_WHITELIST=false
